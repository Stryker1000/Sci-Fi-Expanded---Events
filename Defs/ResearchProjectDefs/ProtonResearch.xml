        <generalTitle>Sci-fi Expanded research projects</generalTitle>
        <generalDescription>Sci-Fi Expanded technologies</generalDescription>
    </ResearchTabDef>

    <ResearchProjectDef>
        <defName>ProtonTechnology</defName>
        <label>Proton Technology</label>
        <description>A prerequisite to researching the technologies of the proton corp.</description>
        <tab>ScifiExpanded</tab>
        <baseCost>1000</baseCost>
        <techLevel>Industrial</techLevel>
        <prerequisites>
            <li>Fabrication</li>
        </prerequisites>
        <discoveredLetterTitle></discoveredLetterTitle>
        <researchViewX>0.00</researchViewX>
        <researchViewY>2.50</researchViewY>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>ProtonRangedGenOne</defName>
        <label>Generation 1 Proton Corp Guns</label>
        <description>The first generation of guns from the Proton Corp. Developed long ago these guns are fairly rudimentary but can still be deadly in the right hands.</description>
        <tab>ScifiExpanded</tab>
        <baseCost>1200</baseCost>
        <techLevel>Industrial</techLevel>
        <prerequisites>
            <li>ProtonTechnology</li>
            <li>PrecisionRifling</li>
        </prerequisites>
        <researchViewX>1.00</researchViewX>
        <researchViewY>0</researchViewY>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>ProtonMeleeGenOne</defName>
        <label>Generation 1 Proton Corp Melee Weapons</label>
        <description>The first generation of melee weapons from the Proton Corp. Developed long ago these weapons are fairly rudimentary but can still be deadly in the right hands.</description>
        <tab>ScifiExpanded</tab>
        <baseCost>1200</baseCost>
        <techLevel>Industrial</techLevel>
        <prerequisites>
            <li>ProtonTechnology</li>
        </prerequisites>
        <researchViewX>1.00</researchViewX>
        <researchViewY>5.00</researchViewY>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>ProtonArmorGenOne</defName>
        <label>Generation 1 Proton Corp Armor</label>
        <description>The first generation of armor from the Proton Corp. Developed long ago this armor is fairly rudimentary but can still protect your colonists.</description>
        <tab>ScifiExpanded</tab>
        <baseCost>1200</baseCost>
        <techLevel>Industrial</techLevel>
        <prerequisites>
            <li>ProtonTechnology</li>
            <li>FlakArmor</li>
        </prerequisites>
        <researchViewX>1.00</researchViewX>
        <researchViewY>2.50</researchViewY>
    </ResearchProjectDef>

